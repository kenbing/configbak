xterm.termName: xterm-256color
!xterm.geometry: 80x36
xterm*inputMethod: fcitx
xterm*eightBitInput: false
xterm*altSendsEscape: true
xterm*metaSendEscape: true
xterm*deleteIsDEL: true
!xterm*translations: #override \n\
!Shift <KeyRelease>:insert-seven-bit()

xterm*locale: true
xterm.utf8:true
xterm*utf8Title: true
xterm.vt100.utf8fonts.font:DejaVuSansMono Fallback:antialias=true:pixelsize=15
xterm*preeditType: Root
xterm*fontMenu*fontdefault*label: Default
xterm*faceName:DejaVuSansMono:antialias=true:pixelsize=15
xterm*faceNameDoublesize:DejaVuSansMono Fallback:antialias=true:pixelsize=15

xterm.cjkWidth: true 
xterm.xftAntialias: true
xft.dpi: 96
xft.hinting: 1
xft.hintstyle: hintslight
xft.antialias: 1
xft.rgba: rgb

xterm*SaveLines: 1000
xterm*background: #4D4D4D
xterm*foreground: gray90
xterm*scrollbar: false
!xterm*rightScrollBar:true
!xterm*loginshell:  true
xterm*cursorBlink: true
xterm*borderLess: true
xterm*colorUL: yellow
xterm*colorBD: white

!--tango color schema
*xterm*color0: #2e3436
*xterm*color1: #cc0000
*xterm*color2: #4e9a06
*xterm*color3: #c4a000
*xterm*color4: #3465a4
*xterm*color5: #75507b
*xterm*color6: #0b939b
*xterm*color7: #d3d7cf
*xterm*color8: #555753
*xterm*color9: #ef2929
*xterm*color10: #8ae234
*xterm*color11: #fce94f
*xterm*color12: #729fcf
*xterm*color13: #ad7fa8
*xterm*color14: #00f5e9
*xterm*color15: #eeeeec
